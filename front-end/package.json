    "ts-jest": "^29.0.5",
    "ts-migrate": "^0.1.35",
    "ts-node": "^10.9.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/index.tsx",
      "!src/react-app-env.d.ts",
      "!src/**/index.ts",
      "!src/hooks/**",
      "!src/mock/**",
      "!src/testUtils/**",
      "!src/typechain-types/**",
      "!src/types/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "testMatch": [
      "**/*.test.[jt]s?(x)"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@ngrx|(?!deck.gl)|ng-dynamic)"
    ]
  }
}
